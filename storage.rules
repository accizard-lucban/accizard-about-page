rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to news.json
    match /news.json {
      allow read: if true;
      allow write: if false; // Only Cloud Functions can write
    }
    
    // Deny all other access
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
